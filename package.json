{
  "name": "orchestrator-pp-core",
  "version": "4.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruslanpetunin/orchestrator-pp-core"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean"
